  "type": "library",
  "license": "MIT",
  "keywords": [
    "json",
    "api",
    "server"
  ],
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "marien@eosnewmedia.de"
    },
    {
      "name": "Chris Doehring",
      "email": "info@chrisdoehring.de"
    }
  ],
  "autoload": {
    "psr-4": {
